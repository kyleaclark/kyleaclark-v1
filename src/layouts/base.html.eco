<!DOCTYPE html>
<!--[if !IE]><!-->
  <html dir="ltr" lang="en-US" class="no-js" id="ng-app" data-ng-app="PortfolioApp">
<!--<![endif]-->

<!--[if IE 8 ]><html dir="ltr" lang="en-US" class="no-js ie ie8 lte8 lte9" id="ng-app" data-ng-app="PortfolioApp"><![endif]-->
<!--[if IE 9 ]><html dir="ltr" lang="en-US" class="no-js ie ie9 lte9" id="ng-app" data-ng-app="PortfolioApp"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--><html dir="ltr" lang="en-US" class="no-js" id="ng-app" data-ng-app="PortfolioApp"><!--<![endif]-->
  <head>
    <meta charset="UTF-8" />

    <!-- Site Info
    ================================================== -->
    <title><%= @document.title %></title>
    <meta name="description" content="<%= @document.description %>" />
    <meta name="keywords" content="<%= @document.keywords %>" />

    <!-- Mobile Specific Meta
    ================================================== -->
    <meta name="viewport" id="viewport-meta" content="width=device-width, initial-scale=1">

    <!-- CSS
    ================================================== -->

    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Varela+Round' rel='stylesheet' type='text/css'>

    <!-- Styles -->
    <%- @getBlock('styles').toHTML() %>
   
    <!--[if lt IE 9]>
      <script src="{{contextRoot}}/lib/html5shiv-3.6.2.js"></script>
    <![endif]-->
  </head>
  <body class="<%= @document.body_class %>">

    <%- @content %>

     <!-- JS
    ================================================== -->
    <%- @partial "framework.html", @ %>
    <%- @partial "ga.html", @ %>

  </body>
</html>